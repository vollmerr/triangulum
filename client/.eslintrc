{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
      "react/jsx-filename-extension": 0,
      "react/jsx-curly-brace-presence": 0,
      "react/prop-types": 0,
      "jsx-quotes": 0
